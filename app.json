{
  "expo": {
    "name": "TripPlannerV2",
    "slug": "trip-planner-v2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tripplanner.bikecamp",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to find nearby campgrounds and plan your bike camping routes.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses location to find nearby campgrounds and plan your bike camping routes."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tripplanner.bikecamp",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "scheme": "tripplanner",
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow TripPlanner to use your location to find nearby campgrounds and plan routes."
        }
      ],
      "expo-sqlite",
      "expo-secure-store",
      "expo-font"
    ]
  }
}
